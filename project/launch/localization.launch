<?xml version="1.0"?>

<launch>

  <node name="detectmarkers" pkg="project" type="detectmarkers.py" />
  <node name="displaymarkers" pkg="project" type="displaymapmarkers.py" args="$(find dd2419_resources)/worlds_json/test.world.json"/>
  <node name="localization" pkg="project" type="localization.py" />

  <node name="tf_base2camera" pkg="tf2_ros" type="static_transform_publisher" args="0.01 0 0.02 -1.57 0 -1.57 cf1/base_link cf1/camera_link" />
  <node name="tf_map2odom" pkg="tf2_ros" type="static_transform_publisher" args="1.0 0.0 0 0 0 0 map cf1/odom" />
</launch>
